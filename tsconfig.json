{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "target": "es2020",
    "strict": true,
    "sourceMap": false,
    "out": "./index.js",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,

  }
}
